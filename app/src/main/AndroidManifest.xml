<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icetea09.demopropertyanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ValueAnimatorActivity"
            android:label="@string/value_animator" />

        <activity
            android:name=".ObjectAnimatorActivity"
            android:label="@string/object_animator" />

        <activity
            android:name=".AnimatorSetActivity"
            android:label="@string/animator_set" />

        <activity
            android:name=".InterpolatorActivity"
            android:label="@string/interpolator" />

        <activity
            android:name=".XmlActivity"
            android:label="@string/xml" />

    </application>

</manifest>
